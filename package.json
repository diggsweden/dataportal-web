{
  "name": "dataportal-web",
  "version": "4.0.3",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "react-env --env local -- next start",
    "check-types": "tsc --noemit",
    "lint": "yarn check-types && next lint",
    "graphql:introspect": "apollo service:download graphql/schema.json",
    "graphql:generate": "apollo codegen:generate --addTypename --target=typescript --globalTypesFile=\"graphql/__generated__/globalTypes.ts\"",
    "graphql": "yarn graphql:introspect && yarn graphql:generate",
    "image": "docker build . -t dataportal-web"
  },
  "dependencies": {
    "@alfalab/winston3-logstash-transport": "^1.3.0",
    "@apollo/client": "^3.6.9",
    "@beam-australia/react-env": "^3.1.1",
    "@datapunt/matomo-tracker-react": "^0.5.1",
    "@digg/design-system": "^4.3.0",
    "@types/node-fetch": "^2.6.2",
    "@types/swagger-ui-react": "^4.11.0",
    "fetch-enhanced": "^8.1.1",
    "focus-trap-react": "^10.0.0",
    "graphql": "16.4.0",
    "https-proxy-agent": "^5.0.1",
    "lucene": "^2.1.1",
    "next": "12.3.0",
    "next-absolute-url": "^1.2.2",
    "next-translate": "^1.5.0",
    "node-fetch": "^3.2.10",
    "prismjs": "^1.29.0",
    "qss": "^2.0.3",
    "react": "18.2.0",
    "react-children-utilities": "^2.8.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.3",
    "react-show-more-text": "^1.5.2",
    "react-vis": "^1.11.7",
    "rehype-raw": "^6.1.1",
    "remark-gfm": "^3.0.1",
    "sharp": "^0.31.0",
    "swagger-ui-react": "4.14.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@emotion/babel-preset-css-prop": "^11.10.0",
    "@types/graphql": "^14.5.0",
    "@types/node": "18.7.16",
    "@types/prismjs": "^1.26.0",
    "@types/react": "18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/react-truncate": "^2.3.4",
    "@types/react-vis": "^1.11.11",
    "apollo": "^2.34.0",
    "eslint": "8.23.0",
    "eslint-config-next": "12.3.0",
    "sass": "^1.56.1",
    "typescript": "4.8.3"
  },
  "engines": {
    "npm": ">=8.1.2",
    "node": ">=16.13.2"
  },
  "resolutions": {
    "**/**/@braintree/sanitize-url": ">=6.0.0",
    "**/**/ansi-regex": "^5.0.1",
    "**/**/minimist": "^1.2.6",
    "**/**/moment": "^2.29.4",
    "**/**/nth-check": "^2.0.1",
    "**/**/parse-path": "^5.0.0",
    "**/**/parse-url": ">=8.1.0",
    "**/**/prismjs": "^1.28.0",
    "**/**/shelljs": "^0.8.5"
  }
